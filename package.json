{
  "name": "base-cwd",
  "description": "Base plugin that adds a getter/setter for the current working directory.",
  "version": "0.2.1",
  "homepage": "https://github.com/node-base/base-cwd",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "node-base/base-cwd",
  "bugs": {
    "url": "https://github.com/node-base/base-cwd/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "is-valid-app": "^0.1.1"
  },
  "devDependencies": {
    "base": "^0.11.0",
    "gulp-format-md": "^0.1.9",
    "mocha": "^2.5.3"
  },
  "keywords": [
    "app",
    "base",
    "baseplugin",
    "cwd"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "base",
        "base-option",
        "base-pipeline",
        "base-plugins",
        "base-task"
      ]
    }
  }
}
