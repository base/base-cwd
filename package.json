{
  "name": "base-cwd",
  "description": "Base plugin that adds a getter/setter for the current working directory.",
  "version": "0.1.3",
  "homepage": "https://github.com/jonschlinkert/base-cwd",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/base-cwd",
  "bugs": {
    "url": "https://github.com/jonschlinkert/base-cwd/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "base": "^0.7.2",
    "gulp-format-md": "^0.1.5",
    "mocha": "^2.3.4"
  },
  "keywords": [
    "base",
    "baseplugin",
    "app"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "base",
        "base-options",
        "base-pipeline",
        "base-plugins",
        "base-tasks"
      ]
    }
  }
}
