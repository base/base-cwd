{
  "name": "base-cwd",
  "description": "Base plugin that adds a getter/setter for the current working directory.",
  "version": "0.1.4",
  "homepage": "https://github.com/jonschlinkert/base-cwd",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/base-cwd",
  "bugs": {
    "url": "https://github.com/jonschlinkert/base-cwd/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "devDependencies": {
    "base": "^0.8.1",
    "base-option": "^0.7.0",
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5"
  },
  "keywords": [
    "app",
    "base",
    "baseplugin",
    "cwd"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "list": [
        "base",
        "base-options",
        "base-pipeline",
        "base-plugins",
        "base-tasks"
      ]
    }
  }
}
